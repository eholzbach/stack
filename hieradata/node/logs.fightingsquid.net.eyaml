classes:
  - nrpe::services::splunk
  - ops_firewall
  - rsyslog::server
  - splunk

ops_firewall::rules:
  '61 allow to splunk web':
    port:
      - 8000
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept
  '62 allow to syslog server':
    port:   514
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept
  '63 allow to syslog':
    port:
      - 514 
    proto:  tcp 
    source: ENC[PKCS7,]
    action: accept
  '64 allow to syslog':
    port:
      - 514 
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept
  '65 allow to syslog':
    port:
      - 514 
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept
  '66 allow to syslog':
    port:
      - 514
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept
  '67 allow to syslog':
    port:
      - 514 
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept
  '68 allow to syslog':
    port:
      - 514 
    proto:  tcp
    source: ENC[PKCS7,]
    action: accept

rsyslog::server::enable_relp: false
rsyslog::server::enable_tcp:  true 

splunk::params::version:  '6.2.3'
splunk::params::build:    '264376'
splunk::params::src_root: 'puppet:///modules/splunk'

